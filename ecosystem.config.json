{
  "apps": [{
    "name": "rfid-api",
    "script": ".venv/bin/gunicorn",
    "args": "wsgi:app --bind 0.0.0.0:5000 --workers 4",
    "interpreter": null,
    "cwd": "./",
    "env": {
      "PYTHONUNBUFFERED": "1"
    },
    "error_file": "logs/error.log",
    "out_file": "logs/access.log",
    "log_date_format": "YYYY-MM-DD HH:mm:ss"
  }]
}