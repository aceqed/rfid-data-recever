{
  "apps": [{
    "name": "rfid-api",
    "script": ".venv/Scripts/gunicorn",
    "args": "main:app -c gunicorn.conf.py",
    "instances": 1,
    "autorestart": true,
    "watch": false,
    "time": true,
    "error_file": "logs/error.log",
    "out_file": "logs/output.log",
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "env": {
      "NODE_ENV": "production",
      "PYTHONUNBUFFERED": "1"
    }
  }]
}